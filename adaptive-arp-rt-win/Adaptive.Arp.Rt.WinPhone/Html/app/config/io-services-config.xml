<?xml version="1.0" encoding="UTF-8"?>
<io-services-config>

<services>
    
    <service name="iWorkspaceSrv" type="XMLRPC_JSON">
		<end-point host="https://workspace.gft.com"
			path="/iworkspacesrv/jsonrpc"
			scheme="/iworkspacesrv/scheme"/>
	</service>
	
	<service name="iWorkspaceSrv" type="XMLRPC_XML">
		<end-point host="https://workspace.gft.com"
			path="/iworkspacesrv/xmlrpc"
			scheme="/iworkspacesrv/scheme"/>
	</service>
    
</services>


</io-services-config>